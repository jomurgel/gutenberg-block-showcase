{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "gbs/post-term-selector",
	"title": "GBS Post Terms",
	"category": "showcase",
	"description": "Display a post's terms.",
	"keywords": [ "tag", "post", "terms", "taxonomy", "selector" ],
	"textdomain": "gutenberg-block-showcase",
	"attributes": {
		"term": {
			"enum": [
				"category",
				"post_tag"
			]
		},
		"isLocked": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"align": false,
		"anchor": false,
		"className": true,
		"customClassName": false,
		"html": false,
		"inserter": true,
		"multiple": true,
		"reusable": false
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css"
}

{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "gbs/top-block",
	"title": "Top Block",
	"category": "showcase",
	"icon": "editor-kitchensink",
	"description": "A container block that holds the blocks that appear at the top of a post.",
	"textdomain": "gutenberg-block-showcase",
	"attributes": {
		"lock": {
			"type": "object",
			"default": {
				"move": true,
				"remove": true
			}
		}
	},
	"supports": {
		"anchor": false,
		"customClassName": false,
		"html": false,
		"multiple": false,
		"inserter": false
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css"
}
